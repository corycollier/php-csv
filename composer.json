{
    "name"              : "corycollier/php-csv",
    "description"       : "Parses CSV files in php, for multiple uses",
    "license"           : "MIT",
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "authors"     : [
        {
            "name"     : "Cory Collier",
            "email"    : "corycollier@corycollier.com",
            "role"     : "developer",
            "homepage" : "http://corycollier.com/"
        }
    ],
    "require-dev"           : {
        "phploc/phploc"             : "^3.0@dev",
        "pdepend/pdepend"           : "~2.0",
        "phpmd/phpmd"               : "dev-master",
        "squizlabs/php_codesniffer" : "^2.0@dev",
        "sebastian/phpcpd"          : "^2.0@dev",
        "phpunit/phpunit"           : "~5.0",
        "theseer/phpdox"            : "dev-master",
        "satooshi/php-coveralls"    : "^0.7.1"
    },
    "require" : {
        "corycollier/php-cli" : "dev-master"
    },

    "autoload": {
        "psr-4": { "PhpCsv\\": "src/" }
    }
}
